alias awsqa="authenticate -u dpearce -h idp.realestate.com.au -r IS-QA-Administrator"
alias awsdev="authenticate -u dpearce -h idp.realestate.com.au -r IS-Dev-Administrator"

eval "$(boot2docker shellinit 2>/dev/null)"
